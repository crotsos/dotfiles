text/html;              w3m -dump %s; nametemplate=%s.html; copiousoutput
application/*;		~/bin/view_attachment.sh %s -
image/*; 		~/bin/view_attachment.sh %s -

audio/*; ~/bin/view_attachment.sh %s

image/*; ~/bin/view_attachment.sh %s

application/msword; ~/bin/view_attachment.sh  %s
application/pdf; ~/bin/view_attachment.sh %s
application/postscript ; ~/bin/view_attachment.sh %s

text/html;  /usr/bin/w3m -dump -T text/html -I %{charset} -O utf-8 %s; copiousoutput; description=HTML Text; nametemplate=%s.html
Text; nametemplate=%s.html

